<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_customer" model="ir.sequence">
            <field name="name">Sequence Customer</field>
            <field name="code">SEQ_CUSTOMER</field>
            <field name="prefix">KH</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_order" model="ir.sequence">
            <field name="name">Sequence Order</field>
            <field name="code">SEQ_ORDER</field>
            <field name="prefix">HD</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_employee" model="ir.sequence">
            <field name="name">Sequence Employee</field>
            <field name="code">SEQ_EMPLOYEE</field>
            <field name="prefix">NV</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
